# AWS RDS Configuration
RDS_HOST=your-rds-endpoint.rds.amazonaws.com
RDS_PORT=5432
RDS_DATABASE=contoso_db
RDS_USER=your-username
RDS_PASSWORD=your-password

# AWS Credentials (optional if using IAM roles)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# SSH Tunnel Configuration (for secure access to private RDS)
# Set USE_SSH_TUNNEL=true to enable SSH tunnel
USE_SSH_TUNNEL=false
SSH_HOST=your-ec2-public-ip-or-hostname
SSH_USER=ec2-user
SSH_KEY_PATH=~/.ssh/your-key.pem
SSH_LOCAL_PORT=5433
